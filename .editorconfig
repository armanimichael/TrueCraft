root=true

[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
charset = utf-8
trim-trailing-whitespace = true

[*.cs]
indent_size = 4
tab_width = 4
csharp_new_line_before_open_brace = all
csharp_indent_block_contents = true
csharp_indent_braces = false

[*.xml]
indent_size = 2
tab_width = 2

